<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <Label fx:id="title" styleClass="title" stylesheets="@application.css" text="Recipe Name"/>
    <Button fx:id="backButton" text="Back" layoutX="50" layoutY="30" styleClass="backButton" stylesheets="@application.css"/>
    <Button fx:id="prevButton" text="Prev" layoutY="640.0" styleClass="bottomButton" stylesheets="@application.css"/>
    <Button fx:id="nextButton" text="Next" layoutX="640.0" layoutY="640.0" styleClass="bottomButton" stylesheets="@application.css"/>
</AnchorPane>
